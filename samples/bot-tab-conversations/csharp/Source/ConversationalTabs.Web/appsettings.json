{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AzureAd": {
    "ClientId": "<<app-id>>",
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "common",
    "Audience": "<<app-id>>",
    "ClientSecret": "<<client secret>>"
  },
  "Bot": {
    "MicrosoftAppType": "MultiTenant",
    "MicrosoftAppId": "<<app-id>>",
    "MicrosoftAppPassword": "<<app-client-secret (Obtained from the Azure portal for the bot's registration)>>"
  },
  "Graph": {
    "BaseUrl": "https://graph.microsoft.com/v1.0",
    "Scopes": "https://graph.microsoft.com/.default"
  },
  "HostUrl": "https://<<deployment-url>>"
}
